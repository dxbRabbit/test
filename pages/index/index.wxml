 <view class="container">
        <swiper
          indicator-dots="{{indicatorDots}}"
          autoplay="{{autoplay}}"
          interval="{{interval}}"
          duration="{{duration}}"
          style='height:{{swiper_height}}'
        >
        <block wx:for="{{imgUrls}}">
            <swiper-item>
              <image  src="{{item}}" style='width:100%;150px' />
            </swiper-item>
          </block>
        </swiper>
  </view> 
